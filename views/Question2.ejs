<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <div class="con">
        <h1>Products</h1>
        <table class="tbl">
            <tr>
                <th>Id</th>
                <th>Product Name</th>
                <th>Brand</th>
                <th>Price</th>
            </tr>
            <% if(userData.length !=0){ %>
                <% userData.forEach((results) => { %>
            <tr>
                <td><%= results.id %></td>
                <td><%= results.prodname %></td>
                <td><%= results.brand %></td>
                <td><%= results.price %></td>
            </tr>
            <% }); %>
            <% } else{ %>
            <tr class="table-danger">
                <td colspan="9">No data found</td>
            </tr>
            <% } %> 
        </table>
    </div>
</body>
</html>